                '@AppBundle/Resources/skin/admin/jss/serial.js'
                '@AppBundle/Resources/skin/admin/jss/pie.js'
                '@AppBundle/Resources/skin/admin/jss/radar.js'
                '@AppBundle/Resources/skin/admin/jss/light.js'
                '@AppBundle/Resources/skin/admin/jss/patterns.js'
                '@AppBundle/Resources/skin/admin/jss/chalk.js'
