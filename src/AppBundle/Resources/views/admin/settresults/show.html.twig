    {{ form_start(delete_form) }}
    <div  class="btn-group btn-group-circle btn-group-solid">
        <a href="{{ path('settresults_index') }}"  class="btn green">Back to the list</a>
        <a href="{{ path('settresults_edit', { 'id': settResult.id }) }}" class="btn yellow">Edit</a>
        <input type="submit" value="Delete"  class="btn red">
