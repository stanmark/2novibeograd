{% extends '@AppBundle/Resources/views/admin/table.html.twig' %}

{% block tbody %}
    <h1>Settresult</h1>

    <table  class="table table-striped table-bordered table-hover" id="sample_1">
        <tbody>
            <tr>
                <th>Liga</th>
                <td>{{ settResult.game.round.groupp.league.league }}</td>
            </tr>
            <tr>
                <th>Grupa</th>
                <td>{{ settResult.game.round.groupp.groupp }}</td>
            </tr>
            <tr>
                <th>Kolo</th>
                <td>{{ settResult.game.round.rounds }}</td>
            </tr>
             <tr>
                <th>Domacin</th>
                <td>{{ settResult.game.team1.team }}</td>
            </tr>          
            <tr>
                <th>Pointsnumber1</th>
                <td>{{ settResult.pointsNumber1 }}</td>
            </tr>
            <tr>
                <th>Pointsnumber2</th>
