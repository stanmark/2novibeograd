{% extends 'base.html.twig' %}

{% block body %}
    <h1>Seo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sEO.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ sEO.description }}</td>
            </tr>
            <tr>
                <th>Titletag</th>
                <td>{{ sEO.titletag }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ sEO.title }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if sEO.created %}{{ sEO.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if sEO.updated %}{{ sEO.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('seo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('seo_edit', { 'id': sEO.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
