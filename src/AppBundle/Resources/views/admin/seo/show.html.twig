{% extends '@AppBundle/Resources/views/admin/table.html.twig' %}

{% block tbody %}
    <h1>SEO</h1>

    <table  class="table table-striped table-bordered table-hover" id="sample_1">
        <tbody>          
            <tr>
                <th>Description</th>
                <td>{{ sEO.description }}</td>
            </tr>
            <tr>
                <th>Titletag</th>
                <td>{{ sEO.titletag }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ sEO.title }}</td>
            </tr>          
        </tbody>
    </table>
    {{ form_start(delete_form) }}
    <div  class="btn-group btn-group-circle btn-group-solid">
        <a href="{{ path('seo_index') }}" class="btn green" class="btn yellow">Back to the list</a>
        <a href="{{ path('seo_edit', { 'id': sEO.id }) }}">Edit</a>
        <input type="submit" value="Delete" class="btn red">
    </div>    
    {{ form_end(delete_form) }}

{% endblock %}
