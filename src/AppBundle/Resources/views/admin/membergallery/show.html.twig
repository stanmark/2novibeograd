{% extends '@AppBundle/Resources/views/admin/table.html.twig' %}

{% block thead %}
    <h1>Membergallery</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ memberGallery.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ memberGallery.url }}</td>
            </tr>
            <tr>
                <th>Alt</th>
                <td>{{ memberGallery.alt }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if memberGallery.created %}{{ memberGallery.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if memberGallery.updated %}{{ memberGallery.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('membergallery_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('membergallery_edit', { 'id': memberGallery.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
